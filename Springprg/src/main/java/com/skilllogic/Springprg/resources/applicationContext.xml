<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="emp" class="com.skilllogic.Springprg.beans.Employee">
   <property name="id" value="111"></property>
   <property name="name" value="veeru"></property>
   <property name="salary" value="80000.00"></property>
    <property name="ADDR" ref="Addr"></property>
   
    </bean>
    <bean id="Addr" class="com.skilllogic.Springprg.beans.AddressEmployee">
   <property name="Dno" value="1-17/B"></property>
   <property name="Area" value="jpm"></property>
   <property name="City" value="Khammam"></property>
    </bean>
    </beans>
